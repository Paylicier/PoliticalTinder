{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932B0760P0D1N002812", "chronotag": "1676500306961", "legislature": "16", "identification": {"numeroLong": "2812", "numeroOrdreDepot": "2812", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932B0760P0D1", "texteLegislatifRef": "PRJLANR5L16B0760", "triAmendement": "auaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaiqao", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720430", "groupePolitiqueRef": "PO800490", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA795588", "PA795596", "PA342384", "PA719578", "PA794130", "PA791824", "PA795136"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bernalicis,  Mme&#160;Amrani, M.&#160;Vannier, Mme&#160;&#201;lisa&#160;Martin, M.&#160;Prud&apos;homme, M.&#160;Kerbrat, Mme&#160;Legrain et M.&#160;Coulomme"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/0760.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Au d&#233;but des cinqui&#232;me et neuvi&#232;me alin&#233;as de l&#8217;article L.&#160;137&#8209;11&#8209;1 du code de la s&#233;curit&#233; sociale, le taux&#160;: &#171;&#160;21&#160;%&#160;&#187; est remplac&#233; par le taux&#160;: &#171;&#160;30&#160;%&#160;&#187;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">\"Cet amendement a pour objet d&#8217;augmenter significativement la taxation des retraites chapeau les plus importantes.</p><p>Aujourd&#8217;hui, le taux de la contribution due par les b&#233;n&#233;ficiaires des rentes sup&#233;rieures &#224; 24 000 &#8364; par mois est de 21 %. Cet amendement propose d&#8217;augmenter ce taux &#224; 30 % soit plus de 9 points. Le taux propos&#233; permet non seulement de faire contribuer les plus ais&#233;s selon les moyens dont ils disposent mais aussi de dissuader le recours &#224; ce dispositif pour les rentes tr&#232;s importantes.s</p><p>\"</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2023-02-01", "datePublication": "2023-02-09", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2023-02-15T23:30:03+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932B0760P0D1N002812?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2023IDS26795", "article99": "false", "loiReference": {"codeLoi": "c s\u00e9curit\u00e9 sociale", "divisionCodeLoi": "L137-11-1 5 30"}, "discussionCommune": {"idDiscussion": "20491", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "59514", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}