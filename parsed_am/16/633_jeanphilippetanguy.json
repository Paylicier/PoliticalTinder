"titre": "Article 10", "articleDesignationCourte": "APR\u00c8S ART. 10", "articleDesignation": "APR\u00c8S L'ARTICLE 10, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/0017.asp#D_Article_10"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Le Gouvernement remet un rapport au Parlement avant le 1<sup>er</sup> octobre 2022&#160;pour &#233;tudier si la hausse constat&#233;e des tarifs des mutuelles de sant&#233; depuis cinq ans est r&#233;ellement justifi&#233;e par l&#8217;&#233;volution des co&#251;ts de sant&#233;.</p><p style=\"text-align: justify;\">Une attention particuli&#232;re est port&#233;e aux tarifs appliqu&#233;s aux personnes &#226;g&#233;es de plus de 65&#160;ans ainsi qu&#8217;aux personnes pr&#233;caires.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Depuis plusieurs ann&#233;es, le co&#251;t des mutuelles augmente de mani&#232;re importante pour les Fran&#231;ais, en particulier pour nos ain&#233;s, sans que l'on sache r&#233;ellement si ces hausses sont justifi&#233;es au regard de l'&#233;volution r&#233;elle des co&#251;ts de sant&#233;.</p><p>Un rapport permettra de faire un &#233;tat des lieux et de justifier, le cas &#233;ch&#233;ant, un gel du co&#251;t des mutuelles pour les Fran&#231;ais.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2022-07-18", "datePublication": "2022-07-22", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2022-07-26T23:53:53+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932B0017P0D1N000633?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2022IDS26216", "article99": "false", "loiReference": {"codeLoi": "Z3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}