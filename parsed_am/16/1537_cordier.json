el": "false", "urlDivisionTexteVise": "/16/textes/0760.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p>I.&#160;&#8211;&#160;Les pensions de retraite d&#8217;un montant inf&#233;rieur&#160;ou &#233;gal &#224; l&#8217;allocation de solidarit&#233; aux personnes &#226;g&#233;es sont exon&#233;r&#233;es de pr&#233;l&#232;vements au titre de la contribution sociale g&#233;n&#233;ralis&#233;e&#160;et de la contribution pour le remboursement de la dette sociale.</p><p>II.&#160;&#8211;&#160;La perte de recettes pour les organismes de s&#233;curit&#233; sociale est compens&#233;e &#224; due concurrence par la majoration de l&#8217;accise sur les tabacs pr&#233;vue au chapitre&#160;IV du titre&#160;Ier&#160;du livre&#160;III du code des impositions sur les biens et services.<br /><br /><br /></p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Afin de sauvegarder&#160;le pouvoir d'achat des retrait&#233;s tr&#232;s modestes, cet amendement&#160;propose&#160;d&#8217;exon&#233;rer de la CSG ou de la CRDS les retraites inf&#233;rieures ou &#233;gales au minimum vieillesse.&#160;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2023-02-01", "datePublication": "2023-02-17", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2023-02-17T22:21:40+01:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932B0760P0D1N001537?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2023IDS26804", "article99": "false", "loiReference": {"codeLoi": "Z1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "20598", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "62164", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}